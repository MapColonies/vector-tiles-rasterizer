{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "logger": {
    "level": "LOG_LEVEL"
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  }
}
