{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "logger": {
    "level": "LOG_LEVEL"
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "bodyLimit": "REQUEST_PAYLOAD_LIMIT",
        "parseAs": "REQUEST_PARSE_AS"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "application": {
    "styleFilePath": "APP_STYLE_FILE_PATH",
    "tileSize": {
      "__name": "APP_TILE_SIZE",
      "__format": "number"
    },
    "ratio": {
      "__name": "APP_RATIO",
      "__format": "number"
    },
    "poolResources": {
      "min": {
        "__name": "APP_POOL_RESOURCES_MIN",
        "__format": "number"
      },
      "max": {
        "__name": "APP_POOL_RESOURCES_MAX",
        "__format": "number"
      }
    },
    "zoom": {
      "min": {
        "__name": "APP_ZOOM_MIN",
        "__format": "number"
      },
      "max": {
        "__name": "APP_ZOOM_MAX",
        "__format": "number"
      }
    }
  },
  "tests": {
    "enabled": {
      "__name": "TESTS_ENABLED",
      "__format": "boolean"
    },
    "poolInactivityClose": {
      "__name": "TESTS_POOL_INACTIVITY_CLOSE",
      "__format": "number"
    }
  }
}
