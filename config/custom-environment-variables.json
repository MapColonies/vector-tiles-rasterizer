{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "logger": {
    "level": "LOG_LEVEL"
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "bodyLimit": "REQUEST_PAYLOAD_LIMIT",
        "parseAs": "REQUEST_PARSE_AS"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "application": {
    "styleFilePath": "STYLE_FILE_PATH",
    "tileSize": {
      "__name": "TILE_SIZE",
      "__format": "number"
    },
    "ratio": {
      "__name": "RATIO",
      "__format": "number"
    },
    "pool": {
      "resources": {
        "min": {
          "__name": "MIN_RESOURCES",
          "__format": "number"
        },
        "max": {
          "__name": "MAX_RESOURCES",
          "__format": "number"
        }
      }
    },
    "zoom": {
      "min": {
        "__name": "MIN_ZOOM",
        "__format": "number"
      },
      "max": {
        "__name": "MAX_ZOOM",
        "__format": "number"
      }
    }
  }
}
