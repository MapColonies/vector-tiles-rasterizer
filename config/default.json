{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "logger": {
    "level": "info"
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "bodyLimit": "1mb",
        "parseAs": "string"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  }
}
