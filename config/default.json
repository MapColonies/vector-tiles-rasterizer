{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "logger": {
    "level": "info"
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "bodyLimit": "1mb",
        "parseAs": "string"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "application": {
    "styleFilePath": "/usr/src/app/resources/styles/zurich-style.json",
    "tileSize": 256,
    "ratio": 1,
    "resources": {
      "min": 12,
      "max": 24
    },
    "zoom": {
      "min": 0,
      "max": 22
    }
  }
}
